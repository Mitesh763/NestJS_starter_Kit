 "scripts": {
  "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
  "start": "nest start",
  "start:debug": "nest start --debug --watch",
  "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
  "test": "jest",
  "test:watch": "jest --watch",
  "test:cov": "jest --coverage",
  "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
  "test:e2e": "jest --config ./test/jest-e2e.json",
  "build": "yarn build:assets && nest build",
  "build:assets": "yarn clean:assets && webpack --mode production",
  "build:assets:dev": "webpack --mode development --watch",
  "clean:assets": "rimraf public/assets",
  "start:dev": "concurrently \"yarn build:assets:dev\" \"nest start --watch\"",
  "start:prod": "yarn build && node dist/main",
  "typeorm": "typeorm-ts-node-commonjs"
 },
